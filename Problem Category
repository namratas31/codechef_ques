int main() {
	// your code goes here
	int T,x;
	cin>>T;
	int arr[T];
	
	for(int i=0;i<T;i++)
	{
	    cin>>x;
	    if(x<100)
	    arr[i]=1;
	    else if(x>=100 && x<200)
	    arr[i]=2;
	    else
	    arr[i]=3;
	}
	
	for(auto i:arr)
	{
	    if(i==1)
	    cout<<"Easy"<<endl;
	    else if(i==2)
	    cout<<"Medium"<<endl;
	    else 
	    cout<<"Hard"<<endl;
	}
	
	return 0;
}
